<div class="formb">
    <%= simple_form_for [:artist, @song], method: :post do |f| %>
    <%= f.input :title %>
    <%= f.input :tags %>
    <%= f.input :description %>
    <%= f.input :video, label: 'Sound'%>
    <%= f.input :video_cache, as: :hidden %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>

    <%= f.submit "Save music", class: "btn btn-success" %>
    <% end %>
  </div>



