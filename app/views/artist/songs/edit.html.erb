  <p class="h1 text-center">Add a new song</p>

   <div class="container">
    <%= simple_form_for(@song, url: artist_song_path, method: :patch) do |f| %>
    <%= f.input :title %>
    <%= f.input :tags %>
    <%= f.input :description %>
    <%= f.input :video %>
    <%= f.input :video_cache, as: :hidden %>
    <%= f.input :post_at, as: :string, required: false, input_html: {class: "datepicker"} %>
    <%= f.input :duration %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>

    <%= f.submit "Edit music", class: "btn btn-success" %>
    <% end %>
   </div>
